{
  "name": "umi-electron-boilerplate",
  "version": "0.0.12",
  "description": "umi-electron-boilerplate",
  "main": "dist/main.bundle.js",
  "scripts": {
    "copy_package_json": "node scripts/copy_package_json.js",
    "build:action": "yarn ncc build actions/release/src/index.js -o actions/release/dist",
    "start": "node scripts/lib/start.js ",
    "postinstall": "npm run copy_package_json electron && npm run build:scripts && electron-builder install-app-deps",
    "build:scripts": "rimraf scripts/lib && yarn tsc --build tsconfig.scripts.json",
    "build": "concurrently \"yarn build:main\" \"yarn build:renderer\"",
    "build:renderer": "cross-env APP_ROOT=src/renderer umi build",
    "build:main": "rimraf dist && cross-env NODE_ENV=production webpack --progress --colors && npm run copy_package_json electron",
    "dev": "cross-env NODE_ENV=development webpack --watch --progress --colors",
    "test": "jest",
    "release:win": "electron-builder build --win --x64 -p never",
    "release:mac": "electron-builder build --mac --x64 -p never",
    "pack:win": "node scripts/pack.js",
    "lint": "eslint --ext .js,.ts,.tsx ."
  },
  "egg": {
    "typescript": true
  },
  "build": {
    "electronDownload": {
      "mirror": "https://cdn.npm.taobao.org/dist/electron/"
    },
    "artifactName": "umi-electron-boilerplate.${ext}",
    "appId": "im.umi-electron-boilerplate",
    "productName": "umi-electron-boilerplate",
    "directories": {
      "app": "./dist",
      "output": "./out/"
    },
    "win": {
      "target": "nsis",
      "asar": true
    },
    "nsis": {
      "oneClick": false,
      "perMachine": false,
      "allowElevation": true,
      "allowToChangeInstallationDirectory": true,
      "runAfterFinish": false
    },
    "buildVersion": "0.0.2"
  },
  "dependencies": {
    "@ant-design/dark-theme": "^1.0.3",
    "@ant-design/icons": "^4.0.0-rc.0",
    "@shihengtech/hooks": "^0.0.6",
    "@umijs/plugin-antd": "^0.5.1",
    "@zeit/ncc": "^0.22.0",
    "antd": "^4.1.0",
    "babel-polyfill": "^6.26.0",
    "cheerio": "^1.0.0-rc.3",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.11.0",
    "classnames": "^2.2.6",
    "compressing": "^1.5.0",
    "compression": "^1.7.4",
    "dayjs": "^1.8.21",
    "express": "^4.17.1",
    "fs-extra": "^9.0.0",
    "get-port": "^5.1.1",
    "globby": "^11.0.0",
    "jest": "^25.1.0",
    "kill-port": "^1.6.0",
    "log4js": "^6.2.1",
    "mobx": "^5.15.4",
    "mobx-react": "^6.1.7",
    "multer": "^1.4.2",
    "mz": "^2.7.0",
    "node-disk-info": "^1.0.2",
    "numeral": "^2.0.6",
    "qs": "^6.9.1",
    "rc-resize-observer": "^0.1.3",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-window": "^1.8.5",
    "reflect-metadata": "^0.1.13",
    "routing-controllers": "^0.8.0",
    "sharp": "^0.25.2",
    "sqlite": "^3.0.3",
    "sqlite3": "^4.1.1",
    "typedi": "^0.8.0",
    "umi-request": "^1.2.11"
  },
  "devDependencies": {
    "@actions/core": "^1.2.3",
    "@actions/github": "^2.1.1",
    "@diamondyuan/eslint-config-react-typescript": "^7.0.1",
    "@types/cheerio": "^0.22.16",
    "@types/classnames": "^2.2.9",
    "@types/compression": "^1.7.0",
    "@types/jest": "^25.1.3",
    "@types/koa": "^2.0.52",
    "@types/koa-bodyparser": "^4.3.0",
    "@types/koa-router": "^7.0.42",
    "@types/koa-static": "^4.0.1",
    "@types/lodash": "^4.14.149",
    "@types/mocha": "^7.0.1",
    "@types/multer": "^1.3.10",
    "@types/mz": "^2.7.0",
    "@types/numeral": "^0.0.26",
    "@types/qs": "^6.9.0",
    "@types/react": "^16.7.18",
    "@types/react-dom": "^16.0.11",
    "@types/react-router": "^4.4.3",
    "@types/react-router-dom": "^4.3.1",
    "@types/react-window": "^1.8.1",
    "@types/sharp": "^0.24.0",
    "@types/sqlite3": "^3.1.6",
    "@typescript-eslint/eslint-plugin": "^2.19.0",
    "@typescript-eslint/parser": "^2.19.0",
    "babel-eslint": "^10.0.3",
    "chokidar": "^3.4.0",
    "concurrently": "^5.0.0",
    "copy-webpack-plugin": "^5.1.1",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.1",
    "electron": "^7.1.1",
    "electron-builder": "^22.3.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-import": "^2.15.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^2.2.0",
    "file-loader": "^2.0.0",
    "less": "^3.10.3",
    "less-loader": "^5.0.0",
    "lodash": "^4.17.11",
    "nodemon": "^2.0.2",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.2",
    "style-loader": "^0.23.1",
    "ts-jest": "^25.2.1",
    "ts-loader": "^6.2.2",
    "tsconfig-paths": "^3.7.0",
    "tslint": "^5.12.0",
    "typescript": "^3.7.2",
    "umi": "^3.2.3",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10"
  }
}
